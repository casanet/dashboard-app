{
	"name": "casa.casanet.dashboard",
	"displayName": "casanet",
	"version": "1.0.20",
	"license": "Apache-2.0",
	"main": "index.js",
	"private": false,
	"homepage": "./",
	"scripts": {
		"generate-api": "openapi-generator-cli generate",
		"prebuild": "node scripts/fetch-spec.js && npm run generate-api",
		"prestart": "npm run prebuild",
		"build": "react-scripts build",
		"postbuild": "node scripts/fetch-light-app.js",
		"start": "react-scripts start",
		"run_mock": "node scripts/run-mock-server.js --run",
		"prepare_mock": "node scripts/run-mock-server.js --prepare",
		"serve_mock": "npm run prepare_mock && npm run run_mock",
		"test": "jest --forceExit --maxWorkers 1",
		"eject": "react-scripts eject",
		"patch_version": "npm version --no-git-tag-version patch && node scripts/sync-cordova-version.js",
		"dev_mode": "node scripts/set-dev-variant.js",
		"cordova": "npm run build && cordova run",
		"cordova:android": "npm run build && cordova run android"
	},
	"dependencies": {
		"@date-io/date-fns": "^1.3.13",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@material-ui/core": "^4.12.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/pickers": "^3.2.8",
		"@mui/icons-material": "^5.0.0",
		"@mui/lab": "^5.0.0-alpha.48",
		"@mui/material": "^5.0.0",
		"date-fns": "^2.8.1",
		"i18next": "^20.6.0",
		"isemail": "^3.2.0",
		"javascript-time-ago": "^2.3.10",
		"jss-rtl": "^0.3.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.debounce": "^4.0.8",
		"percent-normalizer": "^1.0.2",
		"react": "^17.0.2",
		"react-color-palette": "^6.1.0",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.11.4",
		"react-router-dom": "^5.3.0",
		"react-time-ago": "^7.1.3",
		"stylis": "^4.0.10",
		"stylis-plugin-rtl": "^2.1.0",
		"ts-events": "^3.4.0",
		"unitsnet-js": "^1.0.25",
		"web-vitals": "^1.0.1"
	},
	"cordova": {
		"plugins": {
			"cordova-plugin-whitelist": {},
			"cordova-plugin-device": {}
		},
		"platforms": [
			"browser",
			"android"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.5.1",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^27.4.0",
		"@types/lodash.clonedeep": "^4.5.6",
		"@types/lodash.debounce": "^4.0.7",
		"@types/node": "^12.0.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.8",
		"cordova-android": "^10.1.1",
		"cordova-browser": "^6.0.0",
		"cordova-plugin-device": "^2.0.3",
		"cordova-plugin-whitelist": "^1.3.5",
		"http-server": "^14.1.0",
		"jest": "^28.1.3",
		"jszip": "^3.7.1",
		"node-fetch": "^3.0.0",
		"puppeteer": "^13.1.1",
		"react-scripts": "4.0.3",
		"sass": "^1.39.0",
		"ts-jest": "^28.0.7",
		"typescript": "^4.1.2",
		"xml-js": "^1.6.11"
	}
}
